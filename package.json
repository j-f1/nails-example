{
  "name": "nails-example",
  "version": "0.5.1",
  "description": "Example Nails app",
  "main": "demo.js",
  "scripts": {
    "test": "xo && echo \"No lint issues!\\nxo ðŸ’š you\"",
    "add-contrib": "all-contributors add",
    "gen-contribs": "all-contributors generate",
    "start": "node demo.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j-f1/nails-example.git"
  },
  "author": "Jed Fox & ArtOfCode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/j-f1/nails-example/issues"
  },
  "homepage": "https://github.com/j-f1/nails-example#readme",
  "xo": {
    "space": true,
    "envs": [
      "node"
    ],
    "rules": {
      "no-var": "error",
      "comma-dangle": ["error", "always-multiline"],
      "no-eq-null": "off",
      "eqeqeq": [
        "error",
        "smart"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "no-multi-assign": "off"
    }
  },
  "devDependencies": {
    "all-contributors-cli": "^5.0.0",
    "xo": "^0.23.0"
  },
  "dependencies": {
    "node-nails": "github:ArtOfCode-/nails"
  }
}
