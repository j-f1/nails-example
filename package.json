{
  "name": "nails-example",
  "version": "0.5.1",
  "description": "Example Nails app",
  "main": "demo.js",
  "scripts": {
    "test": "xo",
    "add-contrib": "all-contributors add",
    "gen-contribs": "all-contributors generate",
    "start": "node demo.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j-f1/nails-example.git"
  },
  "author": "Jed Fox & ArtOfCode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/j-f1/nails-example/issues"
  },
  "homepage": "https://github.com/j-f1/nails-example#readme",
  "xo": {
    "space": true,
    "envs": [
      "node"
    ],
    "overrides": [
      {
        "files": "test/*.js",
        "rules": {
          "camelcase": 0
        }
      }
    ],
    "rules": {
      "no-var": "error",
      "quotes": 0,
      "no-eval": 1,
      "import/no-unassigned-import": 0,
      "import/no-dynamic-require": 0,
      "camelcase": 1,
      "brace-style": 0,
      "no-prototype-builtins": 0,
      "dot-notation": 0,
      "linebreak-style": "off",
      "comma-dangle": "off",
      "no-eq-null": "off",
      "no-loop-func": 0,
      "eqeqeq": [
        "error",
        "smart"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "no-multi-assign": "off",
      "func-names": "off",
      "unicorn/filename-case": "off",
      "unicorn/no-process-exit": 0,
      "max-params": [
        1,
        5
      ]
    }
  },
  "devDependencies": {
    "all-contributors-cli": "^4.0.1",
    "xo": "^0.18.1"
  },
  "dependencies": {
    "node-nails": "^0.5.1"
  }
}
